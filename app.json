      "adaptiveIcon": {
        "foregroundImage": "./assets/iconLogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sunita4u91.sipfund"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
