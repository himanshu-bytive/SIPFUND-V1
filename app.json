{
  "expo": {
    "name": "SIP Fund",
    "slug": "sipfund",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/iconLogo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconLogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sipfund.app",
      "permissions": [
        "LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA_ROLL"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-location"
    ]
  }
}