{
  "expo": {
    "name": "SIP Fund",
    "slug": "sipfund",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconLogo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
